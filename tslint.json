{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "semi": false
      }
    ],
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "jsx-no-multiline-js": false
    "no-lam"
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
